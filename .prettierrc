{
  "endOfLine": "lf",
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "es5",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "^(^react$|@react|react|^recoil$)",
    "^@mui/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^(components|configs|container|layouts|providers|types)(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "singleAttributePerLine": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
